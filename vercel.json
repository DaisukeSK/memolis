{
    "functions": {
      "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      
      
      { "src": "/footer.php", "dest": "/api/login/footer.php" },
      { "src": "/login_check", "dest": "/api/login/login_check.php" },
      { "src": "/function", "dest": "/api/require/function.php" },

      { "src": "/css/login.css", "dest": "/css/login.css" },
      { "src": "/css/common.css", "dest": "/css/common.css" },

      { "src": "/(.*)", "dest": "/api/login/login.php" }
  ]
  }