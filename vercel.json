{
    "functions": {
      "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      { "src": "/require/function.php", "dest": "/api/require/function.php" },
      { "src": "/test", "dest": "/api/folder/test.php" },
      { "src": "/test2", "dest": "/api/folder/test2.php" },

      { "src": "/loginCss", "dest": "/css/login.css" },
      { "src": "/commonCss", "dest": "/css/common.css" },

      { "src": "/login_check", "dest": "/api/login/login_check.php" },

      { "src": "/(.*)", "dest": "/api/login/login.php" }
  ]
  }